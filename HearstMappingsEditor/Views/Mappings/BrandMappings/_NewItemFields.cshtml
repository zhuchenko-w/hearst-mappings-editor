@using HearstMappingsEditor.Models

@model BrandMappingRefs

<label for="nm-project">
    ProjectCode
    <select class="filterable-select project new" id="nm-project">
        @foreach (var item in Model.DimProjects)
        {
            <option value="@item.ProjectID">@($"{item.ProjectCode} ({item.Description})")</option>
        }
    </select>
</label>
<label for="nm-brand">
    LOBDetailCode
    <select class="filterable-select brand new" id="nm-brand">
        @foreach (var item in Model.DimBrands)
        {
            <option value="@item.LOBDetailID">@($"{item.LOBDetailCode} ({item.LOBDetailDesc})")</option>
        }
    </select>
</label>
